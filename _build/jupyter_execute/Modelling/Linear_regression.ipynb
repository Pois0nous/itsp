    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "B = np.random.randn(2,2)\n",
    "x = np.matmul(np.random.randn(100,2),B)\n",
    "\n",
    "X = x[:,0:1].T\n",
    "Y = x[:,1:2].T\n",
    "\n",
    "A = np.matmul(\n",
    "    np.linalg.inv(\n",
    "        np.matmul(X,X.T) ),\n",
    "    np.matmul(X,Y.T) )\n",
    "\n",
    "Yhat = np.matmul(A,X)\n",
    "\n",
    "plt.scatter(X,Y,label='True')\n",
    "plt.scatter(X,Yhat,label='Estimated')\n",
    "plt.xlabel('$X$')\n",
    "plt.ylabel('$Y$')\n",
    "plt.title('Estimating Y from X')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
